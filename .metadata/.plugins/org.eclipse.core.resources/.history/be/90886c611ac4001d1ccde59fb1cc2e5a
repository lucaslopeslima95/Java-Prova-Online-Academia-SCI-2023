import java.util.Scanner;

public class Exercicio02 {
	 public static void main(String[] args) {
	        Scanner scan = new Scanner(System.in);
	        int maior = Integer.MIN_VALUE;
	        int menor = Integer.MAX_VALUE;
	        
	        for (int i = 0; i < 5; i++) {
	            System.out.println("Informe o número " + (i+1) + " de 5");
	            int numeroInserido = scan.nextInt();
	            maior = Math.max(maior, numeroInserido);
	            menor = Math.min(menor, numeroInserido);
	        }
	        
	        System.out.println("O maior número é " + maior + " e o menor número é " + menor);
	        scan.close();
	    }
}
